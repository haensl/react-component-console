{
  "name": "react-component-console",
  "version": "1.2.0",
  "description": "A console emulator component for React.",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run clean && npm run build:module",
    "build:module": "cross-env rollup -c",
    "clean": "rimraf ./dist",
    "coverage": "jest --coverage",
    "lint": "eslint -c .eslintrc.json --ext .jsx,.js src/**/*",
    "lint:ci": "eslint -c .eslintrc.json --ext .jsx,.js --format junit -o test-results/eslint/results.xml src/**/*",
    "prepare": "npm run build",
    "start": "styleguidist server",
    "tdd": "jest --runInBand --watch",
    "test": "jest --runInBand",
    "test:ci": "jest --runInBand --no-cache --ci --reporters='default' --reporters='jest-junit'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "pre-push": "npm run lint && npm test",
      "post-pull": "npm i"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test.setup.js"
    ],
    "clearMocks": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__snapshots__/",
      "<rootDir>/test.setup.js",
      "<rootDir>/styleguide.config.js",
      "<rootDir>/webpack.config.js",
      "<rootDir>/package.json",
      "<rootDir>/package-lock.json"
    ],
    "moduleNameMapper": {
      ".+\\.(css|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "testPathIgnorePatterns": [
      "/package-tests/"
    ]
  },
  "jest-junit": {
    "outputDirectory": "test-results/jest",
    "outputName": "results.xml",
    "suitName": "react-component-console unit tests"
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haensl/react-component-console.git"
  },
  "keywords": [
    "console",
    "emulator",
    "react",
    "react-component",
    "terminal"
  ],
  "author": {
    "name": "HP Dietz",
    "url": "https://hpdietz.com",
    "twitter": "@h_p_d",
    "email": "h.p.dietz@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haensl/react-component-console/issues"
  },
  "homepage": "https://github.com/haensl/react-component-console#readme",
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-flow": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@haensl/eslint-config": "^1.3.0",
    "@haensl/rollup-plugin-local-resolve": "^1.0.6",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^10.0.0",
    "jest-transform-stub": "^2.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-styleguidist": "^10.6.1",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^2.7.1",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-postcss": "^2.0.6",
    "rollup-plugin-terser": "^5.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.5"
  },
  "dependencies": {
    "deepmerge": "^3.3.0",
    "regenerator-runtime": "^0.13.3"
  }
}
