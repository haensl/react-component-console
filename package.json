{
  "name": "react-component-console",
  "version": "0.1.0",
  "description": "A console emulator for React.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint -c .eslintrc.json --ext .jsx,.js src/**/*",
    "start": "styleguidist server",
    "test": "jest --coverage --watch"
  },
  "husky": {
    "hooks": {
      "precommit": "npm run lint"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test.setup.js"
    ],
    "clearMocks": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__snapshots__/",
      "<rootDir>/test.setup.js",
      "<rootDir>/styleguide.config.js",
      "<rootDir>/webpack.config.js",
      "<rootDir>/package.json",
      "<rootDir>/package-lock.json"
    ],
    "moduleNameMapper": {
      ".+\\.(css|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haensl/react-component-console.git"
  },
  "keywords": [
    "console",
    "emulator",
    "react",
    "react-component",
    "terminal"
  ],
  "author": {
    "name": "HP Dietz",
    "url": "https://hpdietz.com",
    "twitter": "@h_p_d",
    "email": "h.p.dietz@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haensl/react-component-console/issues"
  },
  "homepage": "https://github.com/haensl/react-component-console#readme",
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@haensl/eslint-config": "^1.3.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.14.1",
    "eslint-config-react-app": "^3.0.7",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "jest-transform-stub": "^2.0.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-styleguidist": "^9.0.0",
    "react-test-renderer": "^16.8.2",
    "rollup": "^1.2.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.2",
    "webpack": "^4.29.5",
    "webpack-blocks": "^2.0.0-rc"
  }
}
