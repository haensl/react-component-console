{
  "name": "react-component-console",
  "version": "1.1.2",
  "description": "A console emulator for React.",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:module",
    "build:module": "cross-env NODE_ENV=es BABEL_ENV=es rollup -c",
    "clean": "rimraf ./dist",
    "lint": "eslint -c .eslintrc.json --ext .jsx,.js src/**/*",
    "prepare": "npm run build",
    "start": "styleguidist server",
    "test": "jest --coverage",
    "tdd": "jest --coverage --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "pre-push": "npm run lint && npm test"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test.setup.js"
    ],
    "clearMocks": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__snapshots__/",
      "<rootDir>/test.setup.js",
      "<rootDir>/styleguide.config.js",
      "<rootDir>/webpack.config.js",
      "<rootDir>/package.json",
      "<rootDir>/package-lock.json"
    ],
    "moduleNameMapper": {
      ".+\\.(css|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haensl/react-component-console.git"
  },
  "keywords": [
    "console",
    "emulator",
    "react",
    "react-component",
    "terminal"
  ],
  "author": {
    "name": "HP Dietz",
    "url": "https://hpdietz.com",
    "twitter": "@h_p_d",
    "email": "h.p.dietz@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haensl/react-component-console/issues"
  },
  "homepage": "https://github.com/haensl/react-component-console#readme",
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@haensl/eslint-config": "^1.3.0",
    "@haensl/rollup-plugin-local-resolve": "^1.0.5",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.1",
    "jest-transform-stub": "^2.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-styleguidist": "^9.1.16",
    "react-test-renderer": "^16.9.0",
    "rimraf": "^2.7.1",
    "rollup": "^1.21.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.4",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.39.3"
  },
  "dependencies": {
    "deepmerge": "^3.3.0",
    "regenerator-runtime": "^0.13.3"
  }
}
