{
  "name": "react-component-console",
  "version": "1.2.2",
  "description": "A console emulator component for React.",
  "main": "dist/react-component-console.cjs.js",
  "module": "dist/react-component-console.esm.js",
  "unpkg": "dist/react-component-console.umd.js",
  "scripts": {
    "build": "npm run clean && NODE_ENV=production rollup -c",
    "clean": "rimraf ./dist",
    "coverage": "jest --coverage",
    "lint": "eslint -c .eslintrc.json --ext .jsx,.js src/**/*",
    "lint:ci": "eslint -c .eslintrc.json --ext .jsx,.js --format junit -o test-results/eslint/results.xml src/**/*",
    "start": "styleguidist server",
    "tdd": "jest --runInBand --watch",
    "test": "jest --runInBand",
    "test:ci": "jest --runInBand --no-cache --ci --reporters='default' --reporters='jest-junit'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "pre-push": "npm run lint && npm test",
      "post-pull": "npm i"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test.setup.js"
    ],
    "clearMocks": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__snapshots__/",
      "<rootDir>/test.setup.js",
      "<rootDir>/styleguide.config.js",
      "<rootDir>/webpack.config.js",
      "<rootDir>/package.json",
      "<rootDir>/package-lock.json"
    ],
    "moduleNameMapper": {
      ".+\\.(css|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "testPathIgnorePatterns": [
      "/package-tests/"
    ]
  },
  "jest-junit": {
    "outputDirectory": "test-results/jest",
    "outputName": "results.xml",
    "suitName": "react-component-console unit tests"
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haensl/react-component-console.git"
  },
  "keywords": [
    "console",
    "emulator",
    "react",
    "react-component",
    "terminal"
  ],
  "author": {
    "name": "HP Dietz",
    "url": "https://hpdietz.com",
    "twitter": "@h_p_d",
    "email": "h.p.dietz@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haensl/react-component-console/issues"
  },
  "homepage": "https://github.com/haensl/react-component-console#readme",
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@haensl/eslint-config": "^1.3.0",
    "@haensl/rollup-plugin-local-resolve": "^1.0.6",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^10.0.0",
    "jest-transform-stub": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-styleguidist": "^11.0.5",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^2.6.2",
    "rollup-plugin-terser": "^5.3.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "regenerator-runtime": "^0.13.5"
  }
}
